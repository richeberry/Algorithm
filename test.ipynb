   "source": [
    "# 구현 _ 문자열 재정렬\n",
    "import re\n",
    "\n",
    "s = input()\n",
    "num = 0\n",
    "for i in re.findall('\\d',s):\n",
    "    num += int(i)\n",
    "s = ''.join(sorted(re.findall('\\D',s)))\n",
    "print(f'{s}{num}')\n"
   ]
  }
