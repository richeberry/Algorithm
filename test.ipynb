   "source": [
    "# 최소 직사각형\n",
    "\n",
    "data = [[60, 50], [30, 70], [60, 30], [80, 40]]\n",
    "wallet = data[0]\n",
    "\n",
    "for i in data:\n",
    "    if i[0] < i[1]:\n",
    "        i[0], i[1] = i[1], i[0]\n",
    "    wallet[0] = max(wallet[0], i[0])\n",
    "    wallet[1] = max(wallet[1], i[1])\n",
    "\n",
    "print(wallet)"
   ]
  }
