    "# 점수 계산\n",
    "\n",
    "n = int(input())\n",
    "score_lst = list(map(int, input().split()))\n",
    "now = 0\n",
    "score = 0\n",
    "for i in score_lst:\n",
    "    if i == 1:\n",
    "        now += 1\n",
    "        score += now\n",
    "    else:\n",
    "        now = 0\n",
    "\n",
    "print(score)\n"
   ]
